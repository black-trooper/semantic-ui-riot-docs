{
  "name": "semantic-ui-riot-docs",
  "version": "1.0.0",
  "description": "Semantic UI Riot Document",
  "scripts": {
    "devserver": "webpack-dev-server --mode development --inline --watch --hot --content-base ./public/ --port 3002 --open  --history-api-fallback",
    "clean": "rimraf ./src/*hot-update.*",
    "start": "npm run clean && npm run devserver",
    "dev": "webpack --mode development",
    "prod": "webpack --mode production",
    "build": "npm run prod",
    "deploy": "npm run build && firebase deploy"
  },
  "keywords": [
    "riotjs",
    "riot",
    "semantic",
    "semanticui"
  ],
  "author": {
    "name": "black-trooper",
    "url": "https://github.com/black-trooper/semantic-ui-riot-docs/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/black-trooper/semantic-ui-riot-docs.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/black-trooper/semantic-ui-riot-docs/issues"
  },
  "dependencies": {
    "semantic-ui-riot": "0.20.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-es2015-riot": "^1.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.0",
    "escape-html": "^1.0.3",
    "eslint": "^4.18.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "prismjs": "^1.15.0",
    "rimraf": "^2.6.2",
    "riot": "^3.12.0",
    "riot-route": "^3.1.1",
    "riot-tag-loader": "^2.0.2",
    "style-loader": "^0.23.0",
    "vinyl-source-stream": "^2.0.0",
    "webpack": "^4.16.0",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.10",
    "xmp-escape-loader": "^2.1.0"
  }
}
